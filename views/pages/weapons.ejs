<!DOCTYPE html>
<html>

<head>
  <title>CatBot / MHWorld - Monster Hunter World: Iceborne Database</title>
  <link rel="stylesheet" type="text/css" href="../mint.css">
  <link rel="stylesheet" type="text/css" href="../all.min.css">

  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta charset="UTF-8">
  <meta property="og:type" content="website">
  <link rel="icon" href="../icons/catbot.png" />
  <meta name="theme-color" content="#8fde5d" />
  <meta property="og:url" content="https://mhdb.catbot.xyz/" />
  <meta property="og:title" content="CatBot / MHWorld - Monster Hunter World: Iceborne Database" />
  <meta property="og:image" content="https://mhdb.catbot.xyz/icons/catbot.png" />
  <meta property="og:description"
    content="CatBot is a Discord bot dedicated to providing information on Monster Hunter World: Iceborne, CatBot offers information on monsters, weapons, armors, and more!" />
  <meta name="description"
    content="CatBot is a Discord bot dedicated to providing information on Monster Hunter World: Iceborne, CatBot offers information on monsters, weapons, armors, and more!">
  <meta name="keywords"
    content="catbot,discord bot,catbot bot,catbotxyz,discord,catbot mhw,mhw,mhworld,monster hunter,iceborne,monster hunter world,monster hunter world iceborne,database">
  <link rel="shortcut icon" href="/favicon.ico" />
</head>

<body>
  <div class="navigation-bar__light" id="navigation-bar__light">
    <h5><img src="../icons/catbot.png" alt="CatBot" /><a class="navigation-bar__light--link" href="/">CatBot</a></h5>
    <nav>
      <a class="navigation-bar__light--link" href="https://catbot.xyz">Bot</a>
      <a class="navigation-bar__light--link" href="https://discord.gg/srNyk8G">Support</a>
      <div class="toggle-switch__container">
        <label class="toggle-switch" for="checkbox">
          <input class="toggle-switch__input" type="checkbox" id="checkbox" />
          <div class="toggle-switch__slider toggle-switch__round"></div>
        </label>
      </div>
      <form autocomplete="off" id="search-bar" class="form-control__label">
        <div class="dropdown-container">
          <input class="form-control form-control__input-group--none form-control__input-right--margin" type="text"
            id="search-input" placeholder="Search..." title="Type in a name">
          <button class="btn btn-outline__submit form-control__input-group--none" type="submit"><i
              class="fas fa-search"></i></button>
        </div>
      </form>
      <a href="javascript:void(0);" class="icon" onclick="responsiveNavigation()">
        <i class="link-none__no-padding fa fa-bars"></i>
      </a>
    </nav>
  </div>
  <div class="page-container__center">
    <ul class="breadcrumb">
      <li><a href="/">Monster Hunter</a></li>
      <li>Weapons</li>
    </ul>
  </div>
  <div class="page-container__center">
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(great-sword)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('great-sword') %>" alt="great-sword"></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(sword-and-shield)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('sword-and-shield') %>" alt="sword-and-shield" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(dual-blades)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('dual-blades') %>" alt="dual-blades" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(long-sword)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('long-sword') %>" alt="long-sword" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(hunting-horn)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('hunting-horn') %>" alt="hunting-horn" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(gunlance)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('gunlance') %>" alt="gunlance" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(switch-axe)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('switch-axe') %>" alt="switch-axe" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(charge-blade)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('charge-blade') %>" alt="charge-blade" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(insect-glaive)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('insect-glaive') %>" alt="insect-glaive" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(heavy-bowgun)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('heavy-bowgun') %>" alt="heavy-bowgun" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(light-bowgun)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('light-bowgun') %>" alt="light-bowgun" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(bow)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('bow') %>" alt="bow" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(lance)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('lance') %>" alt="lance" /></button>
    <button class="btn btn-bottom__margin-5px btn-outline" onclick="_pageFilter('(hammer)');"><img
        class="table-responsive__img" src="<%- ICON_MAP.get('hammer') %>" alt="hamer" /></button>
  </div>

  <div class="page-container__center page-container__center--flex" id="item-container">
    <% for (let [k, v] of WEAPON_MAP) {%>
    <a class="link-none link-none__blue list-item__center list-item__no-auto" href=/weapons/<%= k %>>
      <% const weapon = WEAPON_MAP.get(k); %>
      <%- "<b>" + weapon.name + "</b>" + "<br>" + "(" + weapon.type + ")" %>
    </a>
    <% } %>
  </div>

  <div class="page-footer">
    <p>Copyright © <a class="link-underline" href="https://ricochhet.github.io/">Ricochet</a>, All Rights Reserved.
      ❤️</p>
  </div>
</body>
<script src="../mint.js"></script>
<script>
  window.onload = function () {
    _pageFilter('(great-sword)');
  }

  _toggleTheme();
  _searchCompletion(`<%- MHW_OBJECTS %>`.split(','), function (input, finalInput) {
    if (decorationArray.includes(input)) { window.location.href = `/decorations/${finalInput.split('/').join('+')}`; }
    if (monsterArray.includes(input)) { window.location.href = `/monsters/${finalInput}`; }
    if (weaponArray.includes(input)) { window.location.href = `/weapons/${finalInput}`; }
    if (armorArray.includes(input)) { window.location.href = `/armors/${finalInput}`; }
    if (skillArray.includes(input)) { window.location.href = `/skills/${finalInput}`; }
    if (itemArray.includes(input)) { window.location.href = `/items/${finalInput}`; }
  });
</script>

</html>