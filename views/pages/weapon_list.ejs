<!DOCTYPE html>
<html>

<head>
  <title>CatBot / MHWorld - Monster Hunter World: Iceborne Database</title>
  <link rel="stylesheet" type="text/css" href="/mint.css">
  <link rel="stylesheet" type="text/css" href="/all.min.css">

  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta charset="UTF-8">
  <meta property="og:type" content="website">
  <link rel="icon" href="/icons/catbot.png" />
  <meta name="theme-color" content="#8fde5d" />
  <meta property="og:url" content="https://mhdb.catbot.xyz/" />
  <meta property="og:title" content="CatBot / MHWorld - Monster Hunter World: Iceborne Database" />
  <meta property="og:image" content="https://mhdb.catbot.xyz/icons/catbot.png" />
  <meta property="og:description"
    content="CatBot is a Discord bot dedicated to providing information on Monster Hunter World: Iceborne, CatBot offers information on monsters, weapons, armors, and more!" />
  <meta name="description"
    content="CatBot is a Discord bot dedicated to providing information on Monster Hunter World: Iceborne, CatBot offers information on monsters, weapons, armors, and more!">
  <meta name="keywords"
    content="catbot,discord bot,catbot bot,catbotxyz,discord,catbot mhw,mhw,mhworld,monster hunter,iceborne,monster hunter world,monster hunter world iceborne,database">
  <link rel="shortcut icon" href="/favicon.ico" />
  <meta name="robots" content="noindex">
</head>

<body>
  <div class="navigation-bar__light" id="navigation-bar__light">
    <h5><img src="/icons/catbot.png" alt="CatBot" /><a class="navigation-bar__light--link" href="/">CatBot</a></h5>
    <nav>
      <a class="navigation-bar__light--link" href="https://catbot.xyz">Bot</a>
      <a class="navigation-bar__light--link" href="https://discord.gg/srNyk8G">Support</a>
      <div class="toggle-switch__container">
        <label class="toggle-switch" for="checkbox">
          <input class="toggle-switch__input" type="checkbox" id="checkbox" />
          <div class="toggle-switch__slider toggle-switch__round"></div>
        </label>
      </div>
      <form autocomplete="off" id="search-bar" class="form-input__label">
        <div class="dropdown-container">
          <input class="form-input form-input__right-margin" type="text"
            id="search-input" placeholder="Search..." title="Type in a name">
          <button class="btn btn-outline__submit form-control__round" type="submit"><i
              class="fas fa-search"></i></button>
        </div>
      </form>
      <a href="javascript:void(0);" class="icon" onclick="responsiveNavigation()">
        <i class="link-none__no-padding fa fa-bars"></i>
      </a>
    </nav>
  </div>
  <div class="page-container__center">
    <ul class="breadcrumb">
      <li><a href="/">Monster Hunter</a></li>
      <li>Weapons</li>
    </ul>
  </div>
  <div class="page-container__center">
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=0"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('great-sword') %>" alt="great-sword"></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=1"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('sword-and-shield') %>" alt="sword-and-shield" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=2"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('dual-blades') %>" alt="dual-blades" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=3"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('long-sword') %>" alt="long-sword" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=4"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('hunting-horn') %>" alt="hunting-horn" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=5"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('gunlance') %>" alt="gunlance" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=6"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('switch-axe') %>" alt="switch-axe" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=7"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('charge-blade') %>" alt="charge-blade" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=8"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('insect-glaive') %>" alt="insect-glaive" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=9"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('heavy-bowgun') %>" alt="heavy-bowgun" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=10"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('light-bowgun') %>" alt="light-bowgun" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=11"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('bow') %>" alt="bow" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=12"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('lance') %>" alt="lance" /></a>
    <a class="btn btn-bottom__margin-5px btn-outline" href="/weapons?id=13"><img class="table-responsive__img"
        src="<%- ICON_MAP.get('hammer') %>" alt="hamer" /></a>
  </div>

  <div class="page-container__center page-container__center--flex" id="item-container">
    <% for (let i in WEAPON_MAP) {%>
    <a class="link-none link-none__blue list-item__center list-item__no-auto"
      href=/mhw/weapons/<%= WEAPON_MAP[i].name.toLowerCase().split(' ').join('') %>>
      <%- WEAPON_MAP[i].name %>
    </a>
    <% } %>
  </div>

  <div class="page-footer">
    <p>Copyright © <a class="link-underline" href="https://ricochhet.github.io/">Ricochet</a>, All Rights Reserved.
      ❤️</p>
  </div>
</body>
<script src="/mint.js"></script>
<script>
  _toggleTheme();
  _searchCompletion(`<%- MHW_OBJECTS %>`.split(','), function (input, finalInput) {
    if (`<%- MHW_DECO_ARRAY %>`.split(',').includes(input)) { window.location.href = `/mhw/decorations/${finalInput.split('/').join('+')}`; }
    if (`<%- MHW_MONSTER_ARRAY %>`.split(',').includes(input)) { window.location.href = `/mhw/monsters/${finalInput}`; }
    if (`<%- MHW_WEAPON_ARRAY %>`.split(',').includes(input)) { window.location.href = `/mhw/weapons/${finalInput}`; }
    if (`<%- MHW_ARMOR_ARRAY %>`.split(',').includes(input)) { window.location.href = `/mhw/armors/${finalInput}`; }
    if (`<%- MHW_SKILL_ARRAY %>`.split(',').includes(input)) { window.location.href = `/mhw/skills/${finalInput}`; }
    if (`<%- MHW_ITEM_ARRAY %>`.split(',').includes(input)) { window.location.href = `/mhw/items/${finalInput}`; }
  });
</script>

</html>